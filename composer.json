{
  "name": "squareetlabs/laravel-toon",
  "description": "LaravelToon - Token-Optimized Object Notation for Laravel. Compress AI prompts, reduce API costs, and optimize LLM contexts seamlessly. Perfect for ChatGPT, Claude, Gemini, and OpenAI integrations.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "laravel-package",
    "laravel-ai",
    "laravel-chatgpt",
    "laravel-gemini",
    "laravel-openai",
    "laravel-anthropic",
    "laravel-mistral",
    "ai",
    "ai-integration",
    "ai-tools",
    "ai-prompt",
    "prompt",
    "prompt-engineering",
    "prompt-optimization",
    "prompt-compression",
    "context-optimizer",
    "context-compression",
    "token",
    "token-efficient",
    "token-optimizer",
    "token-saver",
    "token-counter",
    "llm",
    "llm-tools",
    "openai",
    "chatgpt",
    "claude",
    "anthropic",
    "gemini",
    "mistral",
    "gpt",
    "gpt-4",
    "gpt-4o",
    "ai-developer-tools",
    "json",
    "data",
    "data-formatter",
    "data-compression",
    "data-transformer",
    "compact-json",
    "converter",
    "formatter",
    "parser",
    "serialization",
    "serialization-format",
    "yaml-alternative",
    "object-notation",
    "human-readable-format",
    "php",
    "php8",
    "laravel12",
    "laravel11",
    "laravel10",
    "laravel9",
    "squareetlabs",
    "toon",
    "toon-laravel",
    "laravel-toon"
  ],
  "authors": [
    {
      "name": "Alberto Rial Barreiro",
      "email": "alberto@squareet.com",
      "homepage": "https://www.squareet.com",
      "role": "Developer"
    },
    {
      "name": "Jacobo Cantorna CigarrÃ¡n",
      "email": "jacobo@squareet.com",
      "homepage": "https://www.squareet.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
    "illuminate/console": "^9.0|^10.0|^11.0|^12.0",
    "illuminate/http": "^9.0|^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^1.10",
    "laravel/pint": "^1.10",
    "orchestra/testbench": "^8.0|^9.0",
    "laravel/framework": "^9.0|^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "Squareetlabs\\LaravelToon\\": "src/"
    },
    "files": [
      "src/Helpers/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Squareetlabs\\LaravelToon\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Squareetlabs\\LaravelToon\\LaravelToonServiceProvider"
      ],
      "aliases": {
        "Toon": "Squareetlabs\\LaravelToon\\Facades\\Toon"
      }
    }
  },
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage",
    "format": "pint",
    "analyse": "phpstan analyse",
    "post-autoload-dump": [
      "@php artisan package:discover --ansi"
    ]
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}

